{
  "manifest_version": 2,
  "name": "twitch tv spam",
  "description": "twich tv save spam",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "alarms"
  ],
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": ["http://www.twitch.tv/*"],
      "js": ["js/cs-bundle.js"]
    }
  ],
  "background": {
    "scripts": ["js/bg-bundle.js"]
  }
}
